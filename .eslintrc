{
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "extends": [
      "airbnb-base",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "import",
      "flowtype",
      "unicorn"
    ],
    "rules": {
      "camelcase": "off",
      "no-underscore-dangle": "off",
      "no-param-reassign": "off",
      "unicorn/filename-case": ["error", { "case": "kebabCase" }]
    }
  }
